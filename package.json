{
  "name": "ng2-charts-demo",
  "version": "1.0.0",
  "scripts": {
    "demo.deploy-gh-pages": "gh-pages -d dist/ng2-charts-demo",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --deploy-url /ng2-charts/ --base-href /ng2-charts",
    "build:lib": "ng build --prod ng2-charts",
    "build:schem": "cd ng2-charts-schematics && npm run build && cd ..",
    "build:changelog": "github_changelog_generator -u valor-software -p ng2-charts",
    "install:financial": "git submodule update --init chartjs-chart-financial && cd chartjs-chart-financial && npm i && cd ..",
    "build:financial": "cd chartjs-chart-financial && npm run build && mkdir -p ../dist/chartjs-chart-financial && cp dist/* ../dist/chartjs-chart-financial/ && cd ..",
    "build:financial-on-windows": "cd chartjs-chart-financial && npm run build && mkdir ..\\dist\\chartjs-chart-financial & cp dist/* ../dist/chartjs-chart-financial/ && cd ..",
    "publish:lib": "cd dist/ng2-charts && npm publish && cd ../..",
    "publish:schem": "cd ng2-charts-schematics && npm publish && cd ..",
    "test": "ng test",
    "test:ci": "ng test --watch=false --no-progress --browsers=ChromeHeadless",
    "e2e": "cypress run --spec \"cypress/integration/main-test.js\"",
    "e2e:update-screenshots": "npm run e2e -- --env updateSnapshots=true",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.6",
    "@angular/cdk": "^11.2.5",
    "@angular/common": "^11.2.6",
    "@angular/compiler": "^11.2.6",
    "@angular/core": "^11.2.6",
    "@angular/forms": "^11.2.6",
    "@angular/material": "^11.2.5",
    "@angular/platform-browser": "^11.2.6",
    "@angular/platform-browser-dynamic": "^11.2.6",
    "@angular/router": "^11.2.6",
    "chart.js": "^3.0.0-rc",
    "chartjs-adapter-date-fns": "^1.1.0-beta.1",
    "chartjs-plugin-annotation": "^1.0.0-beta.4",
    "chartjs-plugin-datalabels": "^2.0.0-beta.1",
    "core-js": "^3.9.1",
    "date-fns": "^2.19.0",
    "highlight.js": "^10.6.0",
    "lodash-es": "^4.17.21",
    "ngx-highlightjs": "^4.1.3",
    "ngx-markdown": "^11.1.2",
    "prismjs": "^1.23.0",
    "rxjs": "^6.6.6",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.5",
    "@angular/cli": "^11.2.5",
    "@angular/compiler-cli": "^11.2.6",
    "@angular/language-service": "^11.2.6",
    "@types/jasmine": "^3.6.7",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "^4.17.4",
    "@types/luxon": "^1.26.2",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "cypress": "^6.8.0",
    "cypress-image-snapshot": "^4.0.1",
    "eyes.images": "^3.8.7",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "ng2-charts-schematics": "file:ng2-charts-schematics",
    "ts-node": "^9.0.0",
    "tsickle": "^0.39.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typescript": "~4.1.5",
    "wait-on": "^5.3.0"
  }
}
